{
    "manifest_version": 3,

    "name": "AO3 Kudos-Hits Calculator",
    "version": "0.0.1",
    "description": "Extension to add kudos-hits ratio to ArchiveOfOurOwn.org",

    "content_scripts": [{
      "matches": ["*://archiveofourown.org/*"],
      "js": ["background.js"]
    }],

    "icons": { 
      "16": "icons/16.png",
      "48": "icons/48.png",
      "128": "icons/128.png" 
    }
}